  "config": {
    "path": {
      "prefix": "/"
    }
  },
  "scripts": {
    "prepare": "husky install",
    "setup": "",
    "setup:browser": "opener -a 'Google Chrome Canary' http://localhost:3000$npm_package_config_path_prefix",
    "setup:develop": "NODE_OPTIONS='--inspect' next dev",
    "setup:storybook": "BROWSER='google chrome canary' start-storybook -s ./public -p 6006",
    "setup:lint": "node lint.dev",
    "setup:images": "chokidar --silent true 'public/webps/*' -c 'rimraf public/webps/*.{png,jpg,jpeg,gif}'",
    "dev:path": "pathpida -o ./src/types/ -w",
    "dev": "npm-run-all -p setup:browser setup:develop setup:storybook setup:lint setup:images",
    "build:storybook": "build-storybook -s ./public -o delivery/storybook",
    "build:static": "next build && rimraf public/webps/*.{png,jpg,jpeg,gif} && next export -o delivery/",
    "build:path": "pathpida -o ./src/types/",
    "static": "yarn build:static && yarn build:storybook",
    "clean": "rimraf delivery/* && touch delivery/.gitkeep",
    "jest": "jest --passWithNoTests",
    "build": "next build && rimraf public/webps/*.{png,jpg,jpeg,gif}",
    "start": "next start",
    "serve": "npm-run-all -p setup:browser start",
    "add:fc": "hygen add component",
    "add:page": "hygen add page",
    "add:hooks": "hygen add hooks",
    "add:store": "hygen add store",
    "check-pkg": "ncu",
    "update-pkg": "ncu '/^(?!(three|eslint|@add-module-name)).*$/' -u",
    "re-install": "rm yarn.lock && rm -rf node_modules && yarn install"
  }

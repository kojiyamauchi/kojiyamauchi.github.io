  "main": "index.js",
  "keywords": [
    "gatsby"
  ],
  "dependencies": {
    "@contentful/rich-text-react-renderer": "^14.1.1",
    "@contentful/rich-text-types": "^14.1.1",
    "@fortawesome/fontawesome-svg-core": "^1.2.32",
    "@fortawesome/free-brands-svg-icons": "^5.15.1",
    "@fortawesome/free-regular-svg-icons": "^5.15.1",
    "@fortawesome/free-solid-svg-icons": "^5.15.1",
    "@fortawesome/react-fontawesome": "^0.1.11",
    "babel-plugin-styled-components": "^1.11.1",
    "gatsby": "^2.24.85",
    "gatsby-background-image": "^1.1.2",
    "gatsby-image": "^2.4.21",
    "gatsby-plugin-catch-links": "^2.3.15",
    "gatsby-plugin-exclude": "^1.0.2",
    "gatsby-plugin-graphql-codegen": "^2.7.1",
    "gatsby-plugin-manifest": "^2.4.35",
    "gatsby-plugin-notify": "^1.0.0",
    "gatsby-plugin-offline": "^3.2.37",
    "gatsby-plugin-react-helmet": "^3.3.14",
    "gatsby-plugin-sharp": "^2.6.43",
    "gatsby-plugin-styled-components": "^3.3.14",
    "gatsby-plugin-typescript": "^2.4.24",
    "gatsby-plugin-typescript-checker": "^1.1.1",
    "gatsby-remark-images": "^3.3.39",
    "gatsby-source-contentful": "^3.0.3",
    "gatsby-source-filesystem": "^2.3.35",
    "gatsby-source-google-spreadsheet": "^2.0.0",
    "gatsby-source-microcms": "^1.0.1",
    "gatsby-source-wordpress": "^3.3.39",
    "gatsby-transformer-remark": "^2.8.46",
    "gatsby-transformer-sharp": "^2.5.19",
    "gsap": "^3.5.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0",
    "react-three-fiber": "^5.1.3",
    "styled-components": "^5.2.0",
    "three": "^0.121.1"
  },
  "devDependencies": {
    "@danbruegge/gatsby-plugin-stylelint": "^4.1.0",
    "@types/node": "^14.14.2",
    "@types/react": "^16.9.53",
    "@types/react-dom": "^16.9.8",
    "@types/react-helmet": "^6.1.0",
    "@types/react-reconciler": "^0.18.0",
    "@types/styled-components": "^5.1.4",
    "@types/vfile-message": "^2.0.0",
    "@typescript-eslint/eslint-plugin": "^4.5.0",
    "@typescript-eslint/parser": "^4.5.0",
    "devcert": "^1.1.3",
    "eslint-config-mdcs": "^5.0.0",
    "eslint-config-prettier": "^6.14.0",
    "eslint-plugin-html": "^6.1.0",
    "eslint-plugin-jest": "^24.1.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-promise": "^4.2.1",
    "husky": "^4.3.0",
    "lint-staged": "^10.4.2",
    "npm-check-updates": "^9.1.2",
    "npm-run-all": "^4.1.5",
    "opener": "^1.5.2",
    "prettier": "2.1.2",
    "stylelint": "^13.7.2",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-custom-processor-loader": "^0.6.0",
    "stylelint-processor-styled-components": "^1.10.0",
    "stylelint-webpack-plugin": "^2.1.1",
    "ts-jest": "^26.4.1",
    "typescript": "^4.0.3",
    "wait-on": "^5.2.0"
  },
  "resolutions": {
    "devcert": "1.1.0",
    "remark": "12.0.0",
    "@types/react": "^16.9.53",
    "@types/react-dom": "^16.9.8"
  },
  "browserslist": [
    "last 2 versions",
    "ie >= 11"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged --allow-empty"
    }
  },
  "lint-staged": {
    "src/**/*.{ts,tsx,js,jsx}": ["eslint --fix --max-warnings 0", "stylelint"],
    "gatsby-*.js": "prettier --write --no-semi --single-quote --trailing-comma none --print-width 160",
    "setup.js": "prettier --write --no-semi --single-quote --trailing-comma none --print-width 160"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx"
    ],
    "moduleNameMapper": {
      "^@/(.+)": "<rootDir>/src/$1"
    },
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "tsconfig": "tsconfig.json"
      }
    },
    "testMatch": [
      "**/src/tests/**/*.test.+(ts|tsx|js|jsx)"
    ]
  },
  "scripts": {
    "setup:cms": "rm src/pages/fourth-page.tsx",
    "setup:default": "rm -rf src/templates && rm -rf src/markdowns && rm src/components/Presentations/BlogListLink.tsx && rm src/components/Presentations/BlogListPagenation.tsx && rm src/components/Presentations/BlogPostData.tsx && rm src/components/Presentations/BlogPostPagenation.tsx && rm src/components/Images/MarkdownImage.tsx && sed -e 's/blog-list/fourth-page/' -e 's/Blog Page/Fourth Page/' src/components/Managements/Header.tsx > replace && mv replace src/components/Managements/Header.tsx",
    "setup:browser": "wait-on https://localhost:8000 && opener -a 'Google Chrome Canary' https://localhost:8000 && opener -a 'Google Chrome Canary' https://localhost:8000/___graphql",
    "setup:develop": "gatsby develop --https",
    "dev": "npm-run-all -p setup:browser setup:develop",
    "build": "gatsby build --prefix-paths",
    "serve": "gatsby serve --prefix-paths --open",
    "clean": "gatsby clean",
    "jest": "jest --passWithNoTests",
    "check-pkg": "ncu",
    "update-pkg": "ncu '/^(?!(@add-ignore-module-name)).*$/' -u",
    "re-install": "rm yarn.lock && rm -rf node_modules && yarn install"
  }
}
